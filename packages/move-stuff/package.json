  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "bin": {
    "move-stuff": "./dist/index.cjs",
    "move-shit": "./dist/index.cjs",
    "mvcf": "./dist/index.cjs"
  },
  "scripts": {
    "build": "tsup src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "config",
    "root",
    "organization",
    "move",
    "stuff"
  ],
  "author": "nicole hollant <nicolehollant@gmail.com>",
  "homepage": "https://github.com/nicolehollant/move-stuff#readme",
